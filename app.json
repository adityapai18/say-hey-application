{
  "expo": {
    "name": "Say Hey",
    "slug": "say-hey-app",
    "owner": "sayheyofficial21",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app_icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F8FA"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow SayHey to access your photos"
        }
      ]
    ],
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sayhey.app",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.669318155909-d6ojqal5ii53mroeirmjk6j75tllna5r"
        }
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app_icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.sayhey.app",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
